# Ignore directories generated by Composer or other build processes
/bin/
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/build/pantheon/*

# Ignore some environment variables used for builds and stuff.
/.env

# Ignore Drupal's site specific files
/web/sites/*/files/
/web/sites/*/settings.php

# Ignore non-docroot site specific files
/sites/*/web.sqlite*

# Ignore drush rc / qd files
/web/drush

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest/

# Ignore files generated by PhpStorm and OSX
/.idea/
.DS_Store
